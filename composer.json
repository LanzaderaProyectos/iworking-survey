{
  "name": "iworking/iworking-survey",
  "description": "Create surveys inside your Laravel app",
  "keywords": [
    "laravel",
    "survey"
  ],
  "homepage": "https://github.com/LanzaderaProyectos/iworking-survey",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Nico Penic",
      "email": "ncanteli@strateying.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^6.0|^7.0|^8.0|^9.0",
    "livewire/livewire": "^2.10.6",
    "spatie/laravel-translatable": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "illuminate/http": ">=6",
    "mockery/mockery": "^1.3.2|^1.4",
    "laravel/legacy-factories": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "MattDaneshvar\\Survey\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MattDaneshvar\\Survey\\Tests\\": "tests/"
    },
    "files": [
      "tests/utilities/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "MattDaneshvar\\Survey\\SurveyServiceProvider"
      ]
    }
  }
}